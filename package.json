  "devDependencies": {
    "@testing-library/react": "^10.0.6",
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "badgen": "^3.1.0",
    "chai": "^4.2.0",
    "fs-extra": "^9.0.0",
    "jest": "^26.0.1",
    "lodash.isplainobject": "^4.0.6",
    "lodash.toarray": "^4.4.0",
    "mallery": "1.0.0-alpha.4",
    "microbundle": "developit/microbundle#master",
    "preact": "^10.4.4",

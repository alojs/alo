  "dependencies": {},
  "files": [
    "lib",
    "dist",
    "devtools/package.json",
    "devtools/lib",
    "devtools/dist",
    "preact/package.json",
    "preact/lib",
    "preact/dist",
    "react/package.json",
    "react/lib",
    "react/dist",
    "store/package.json",
    "store/lib",
    "store/dist",
    "selector/package.json",
    "selector/lib",
    "selector/dist",
    "redom/package.json",
    "redom/lib",
    "redom/dist"
  ],
  "standard-version": {
    "scripts": {
      "postchangelog": "node scripts/badges.js && node scripts/site.js && git add assets site README.md"
    }
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "main.(?:tsx|ts|jsx|js)",
      "/lib/tpl-lib/",
      "/node_modules/",
      "/lib/examples/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{ts,tsx,js,jsx}",
      "store/lib/**/*.{ts,tsx,js,jsx}",
      "react/lib/**/*.{ts,tsx,js,jsx}"
    ]
  }
